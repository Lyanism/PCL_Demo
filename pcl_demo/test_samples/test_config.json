{
	"input":{
		"description":"type can be panasonic|pcd...", 
		"type":"panasonic",
		"file":"../test_samples/test_box_up.raw"
	},

	"proc":[	
		{
			"type": "denoise",
			"neighbour": 150, 
			"std_err": 0.1 
		},
		


		{
			"type": "linear-filter",
			"field-name":"z",
			"min": 0,
			"max": 0.8 
		},

		
		{

			"type":"region-growth",
			"norm-neighbour":400,
			"min-points":50,
			"neighbour":50,
			"theta-threshold": 4.0, 
			"curvature-threshold": 0.8 
		
		}

		
	],

	"output":{
		"file":"display"
	}
}
